<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Clicker.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:Clicker.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Clicker">

        <ShellContent
            Title="Clicker"
            ContentTemplate="{DataTemplate view:MainPage}"
            Route="MainPage" />

        <ShellContent
            Title="Add New Clicker"
            ContentTemplate="{DataTemplate view:AddNewClickerPage}"
            Route="AddNewClickerPage" />

    

</Shell>
